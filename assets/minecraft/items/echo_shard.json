{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "id-echo-special",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/echo_shard_special"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/echo_shard"
    }
  }
}
